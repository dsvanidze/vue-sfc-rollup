{
  "name": "sfc-rollup-vuetify",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/sfc-rollup-vuetify.ssr.js",
  "module": "dist/sfc-rollup-vuetify.esm.js",
  "browser": "dist/sfc-rollup-vuetify.esm.js",
  "unpkg": "dist/sfc-rollup-vuetify.min.js",
  "files": [
    "dist/*",
    "sfc-rollup-vuetify.d.ts",
    "src/**/*.vue"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "rollup-plugin-vuetify": "^0.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typescript": "^3.8.3",
    "vee-validate": "^3.3.2",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.30",
    "vuetify-loader": "^1.3.0"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vuetify": "^2.2.11",
    "vee-validate": "^3.3.2"
  },
  "engines": {
    "node": ">=10"
  },
  "types": "sfc-rollup-vuetify.d.ts"
